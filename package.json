    "export": "PATH_TO_ENV_FILE=.env.local node ./contentful/export.js",
    "build-and-setup": "PATH_TO_ENV_FILE=.env node ./contentful/setup.js && next build",
    "deploy-worker": "wrangler publish edge-worker/src/index.ts "
  },
